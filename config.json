{
  "name": "SMS UPS BETA",
  "version": "0.27",
  "slug": "hassio_sms_ups",
  "description": "Read data from SMS BRASIL UPS",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/dmslabsbr/smsUps",
  "startup": "application",
  "boot": "auto",
  "ports": {
    "8000/tcp": 8000
  },
  "devicetree": true,
  "devices": ["/dev/ttyUSB0", "/dev/ttyAMA0"],
  "webui": "[PROTO:use_ssl]://[HOST]:[PORT:8000]/",
  "panel_icon": "mdi:cloud",
  "panel_title": "SMS BRASIL UPS",
  "panel_admin": true,
  "options": {
    "MQTT_HOST": "192.168.50.20",
    "MQTT_USER": "daniel",
    "MQTT_PASS": "mqtt_pass",
    "UPS_NAME": "SMS",
    "UPS_ID": "01",
    "SMSUPS_SERVER": true,
    "SMSUPS_CLIENTE": true,
    "PORTA": "/dev/ttyUSB0",
    "SHUTDOWN_CMD": ["sudo shutdown -h now", "sudo shutdown now", "systemctl poweroff", "sudo poweroff"],
    "USE_SECRETS": false
  },
  "schema": {
    "MQTT_HOST": "str",
    "MQTT_USER": "str",
    "MQTT_PASS": "password",
    "UPS_NAME": "str",
    "UPS_ID": "str",
    "SMSUPS_SERVER": "bool",
    "SMSUPS_CLIENTE": "bool",
    "PORTA": "str",
    "SHUTDOWN_CMD": "str",
    "USE_SECRETS": "bool"
  }
}
